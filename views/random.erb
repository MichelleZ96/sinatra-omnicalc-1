<h1>Pick a random number</h1>

<form action="/random/results">
  <div>
    <label for="Min">Minimum</label>
    <input id="Min" type="number" name="user_min">
  </div>

  <div>
    <label for="Max">Maximum</label>
    <input id="Max" type="number" name="user_max">
  </div>

  <button>Pick random number</button>
</form>
